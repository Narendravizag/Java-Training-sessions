
package oops;
import java.util.Scanner;
class factorial{
	int n;

	void narendra() {
		n=5;
	}
		void show(){
			int f=1;
			for ( int i =1 ;i<=n; i++)
			f =f * i;
			System.out.println("factorial of  :" +n+ "="+f);
			
		}	
}


public class factorials {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		factorial f1=new factorial();
		f1.narendra();
		f1.show();

	}

}
